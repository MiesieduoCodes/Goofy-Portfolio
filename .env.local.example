# Next.js Environment Variables Example
# Copy this file to .env.local and update with your values

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME=Miesieduo Veria Digital Studio

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_GTM_ID=

# Contact Form (Optional)
CONTACT_EMAIL=hello@miesieduo.veria
CONTACT_FORM_ENDPOINT=

# Image Optimization
NEXT_PUBLIC_IMAGE_DOMAINS=localhost,yourdomain.com

# Development
NODE_ENV=development
